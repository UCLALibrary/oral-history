<%# OVERRIDE: Blacklight v6.23.0 - to add custom search_context for interviewee search results, and custom theming %>
<%# EXACT DUPLICATES app/views/full_text/_previous_next_doc.html.erb & app/views/interviewee/_previous_next_doc.html.erb %>
<% if @search_context[:prev] || @search_context[:next] %>
  <div class='pagination-search-widgets'>

    <div class="page-links">
      <%= link_to_previous_document @search_context[:prev] %> |

      <%= item_page_entry_info %> |

      <%= link_to_next_document @search_context[:next] %>
    </div>
  </div>
<% end %>
<% if current_search_session %>
  <div class="pull-right search-widgets">
    <%= link_back_to_catalog class: 'btn' if @search_context %>
  </div>
<% end %>